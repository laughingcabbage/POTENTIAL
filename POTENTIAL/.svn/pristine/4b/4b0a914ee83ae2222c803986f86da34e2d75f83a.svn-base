package com.hr.eclass.member.domain;

import com.hr.eclass.cmn.DTO;

public class WorkInfoVO extends DTO {
	/**년차 */
	private String workYear; 
	/**근무파트 */
	private String workPart;
	/**기준 출근시간 */
	private String startWorkTime; 
	/**기준 퇴근시간 */
	private String endWorkTime; 
	/**일총근무시간 */
	private String workHoPerDay;
	
	public WorkInfoVO(){}
	
	
	public String getWorkYear() {
		return workYear;
	}
	public void setWorkYear(String workYear) {
		this.workYear = workYear;
	}
	public String getWorkPart() {
		return workPart;
	}
	public void setWorkPart(String workPart) {
		this.workPart = workPart;
	}
	public String getStartWorkTime() {
		return startWorkTime;
	}
	public void setStartWorkTime(String startWorkTime) {
		this.startWorkTime = startWorkTime;
	}
	public String getEndWorkTime() {
		return endWorkTime;
	}
	public void setEndWorkTime(String endWorkTime) {
		this.endWorkTime = endWorkTime;
	}
	public String getWorkHoPerDay() {
		return workHoPerDay;
	}
	public void setWorkHoPerDay(String workHoPerDay) {
		this.workHoPerDay = workHoPerDay;
	}


	@Override
	public String toString() {
		return "WorkInfoVO [workYear=" + workYear + ", workPart=" + workPart + ", startWorkTime=" + startWorkTime
				+ ", endWorkTime=" + endWorkTime + ", workHoPerDay=" + workHoPerDay + "]";
	}

}
